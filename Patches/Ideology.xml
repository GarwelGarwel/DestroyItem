<?xml version="1.0" encoding="utf-8" ?>

<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Ideology</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>/Defs/ThoughtDef[defName="Thought_DestroyedCorpse" or defName="Thought_KnowDestroyedCorpse"]</xpath>
			<value>
				<nullifyingPrecepts>
					<li>Cannibalism_Acceptable</li>
					<li>Cannibalism_Preferred</li>
					<li>Cannibalism_RequiredRavenous</li>
					<li>Corpses_DontCare</li>
				</nullifyingPrecepts>
			</value>
		</match>
	</Operation>

